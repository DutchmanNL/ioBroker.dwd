    "native": {
        "url":      "http://www.dwd.de/DWD/warnungen/warnapp/json/warnings.json",
        "region":   "",
        "land": "",
        "warnings": 1,
        "rainRadar": false
    },
    "objects": [


    ],
    "instanceObjects": [
        {
            "_id":  "warning",
            "type": "channel",
            "common": {
                "role": "forecast",
                "name": ""
            },
            "native": {}
        },
        {
            "_id":  "warning.begin",
            "type": "state",
            "common": {
                "name":  "Warning begin",
                "type":  "string",
                "role": "value.datetime",
                "read":  true,
                "write": false
            },
            "native": {}
        },
        {
            "_id": "warning.end",
            "type": "state",
            "common": {
                "name":  "Warning end",
                "type":  "string",
                "role":  "value.datetime",
                "read":  true,
                "write": false
            },
            "native": {}
        },
        {
            "_id": "warning.severity",
            "type": "state",
            "common": {
                "name":  "Warning severity",
                "type":  "number",
                "role":  "value.severity",
                "read":  true,
                "write": false,
                "states": {
                    "0": "None",
                    "1": "Minor",
                    "2": "Moderate",
                    "3": "Severe",
                    "4": "Extreme"
                }
            },
            "native": {}
        },
        {
            "_id": "warning.level",
            "type": "state",
            "common": {
                "name":  "Warning level",
                "type":  "number",
                "role":  "value.warning",
                "read":  true,
                "write": false,
                "states": {
                    "1": "Preliminary info",
                    "2": "Minor",
                    "3": "Moderate",
                    "4": "Severe",
                    "5": "Extreme"
                }
            },
            "native": {}
        },
        {
            "_id": "warning.type",
            "type": "state",
            "common": {
                "name":  "Warning type",
                "type":  "number",
                "role":  "weather.type",
                "read":  true,
                "write": false,
                "states": {
                    "0": "Thunderstorm",
                    "1": "Wind/Storm",
                    "2": "Rain",
                    "3": "Snow",
                    "4": "Fog",
                    "5": "Frost",
                    "6": "Ice",
                    "7": "Thawing",
                    "8": "Heat",
                    "9": "UV warning"
                }
            },
            "native": {}
        },
        {
            "_id":  "warning.text",
            "type": "state",
            "common": {
                "name":  "Warning text",
                "type":  "string",
                "role":  "weather.title.short",
                "read":  true,
                "write": false
            },
            "native": {}
        },
        {
            "_id":  "warning.headline",
            "type": "state",
            "common": {
                "name":  "Warning headline",
                "type":  "string",
                "role":  "weather.title",
                "read":  true,
                "write": false
            },
            "native": {}
        },
        {
            "_id":  "warning.description",
            "type": "state",
            "common": {
                "name":  "Warning description",
                "type":  "string",
                "role":  "weather.state",
                "read":  true,
                "write": false
            },
            "native": {}
        },
        {
            "_id":  "warning.object",
            "type": "state",
            "common": {
                "name":  "JSON object with warning",
                "type":  "object",
                "role":  "weather.json",
                "read":  true,
                "write": false
            },
            "native": {}
        },
        {
            "_id":  "warning.map",
            "type": "state",
            "common": {
                "name":  "Link to chart",
                "type":  "string",
                "role":  "weather.chart.url",
                "read":  true,
                "write": false
            },
            "native": {}
        }
    ]
}
